<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<?eclipse version="3.0"?>
<plugin>
   <extension
         point="org.knime.workbench.repository.categories">
      <category
            icon="icons/missing.gif"
            level-id="comprehend"
            name="Amazon Comprehend"
            path="/labs/aws/">
      </category>
      <category
            icon="icons/missing.gif"
            level-id="translate"
            name="Amazon Translate"
            path="/labs/aws/">
      </category>
      <category
            icon="icons/missing.gif"
            level-id="personalize"
            name="Amazon Personalize"
            path="/labs/aws/">
      </category>
   </extension>
   <extension point="org.knime.workbench.repository.nodes">
	<node 
      	category-path="/labs/aws/comprehend" 
      	factory-class="org.knime.cloud.aws.mlservices.nodes.comprehend.tagging.syntax.SyntaxTaggerNodeFactory"/>
     <node 
      	category-path="/labs/aws/comprehend" 
      	factory-class="org.knime.cloud.aws.mlservices.nodes.comprehend.tagging.entities.EntityTaggerNodeFactory"/>
     <node 
      	category-path="/labs/aws/comprehend" 
      	factory-class="org.knime.cloud.aws.mlservices.nodes.comprehend.language.ComprehendLanguageNodeFactory"/>
      <node 
      	category-path="/labs/aws/comprehend" 
      	factory-class="org.knime.cloud.aws.mlservices.nodes.comprehend.keyphrases.ComprehendKeyPhrasesNodeFactory"/>
      <node 
      	category-path="/labs/aws/comprehend" 
      	factory-class="org.knime.cloud.aws.mlservices.nodes.comprehend.sentiment.ComprehendSentimentNodeFactory"/>
      <node 
      	category-path="/labs/aws/translate" 
      	factory-class="org.knime.cloud.aws.mlservices.nodes.translate.TranslateNodeFactory"/>
      <node
            category-path="/labs/aws/personalize"
            deprecated="false"
            factory-class="org.knime.cloud.aws.mlservices.nodes.personalize.upload.user.AmazonPersonalizeUploadUserDataNodeFactory">
      </node>
      <node
            category-path="/labs/aws/personalize"
            deprecated="false"
            factory-class="org.knime.cloud.aws.mlservices.nodes.personalize.upload.item.AmazonPersonalizeUploadItemDataNodeFactory">
      </node>
      <node
            category-path="/labs/aws/personalize"
            deprecated="false"
            factory-class="org.knime.cloud.aws.mlservices.nodes.personalize.upload.interaction.AmazonPersonalizeUploadInteractionDataNodeFactory">
      </node>
      <node
            category-path="/labs/aws/personalize"
            deprecated="false"
            factory-class="org.knime.cloud.aws.mlservices.nodes.personalize.train.AmazonPersonalizeCreateSolutionVersionNodeFactory">
      </node>
      <node
            category-path="/labs/aws/personalize"
            deprecated="false"
            factory-class="org.knime.cloud.aws.mlservices.nodes.personalize.deploy.AmazonPersonalizeCreateCampaignNodeFactory">
      </node>
      <node
            category-path="/labs/aws/personalize"
            deprecated="false"
            factory-class="org.knime.cloud.aws.mlservices.nodes.personalize.predict.recommendation.userpersonalization.AmazonPersonalizePredictUserPersonalizationNodeFactory">
      </node>
      <node
            category-path="/labs/aws/personalize"
            deprecated="false"
            factory-class="org.knime.cloud.aws.mlservices.nodes.personalize.predict.recommendation.relateditems.AmazonPersonalizePredictRelatedItemsNodeFactory">
      </node>
      <node
            category-path="/labs/aws/personalize"
            deprecated="false"
            factory-class="org.knime.cloud.aws.mlservices.nodes.personalize.predict.personalizedranking.AmazonPersonalizePersonalizedRankingNodeFactory">
      </node>
   </extension>
   <extension
         point="org.knime.ext.textprocessing.TagSet">
      <TagSet
            TagBuilder="org.knime.cloud.aws.mlservices.utils.tagsets.AmazonEntityTag">
      </TagSet>
   </extension>
</plugin>
